parameters:
  level: max
  paths:
    - app
    - tests
  parallel:
    processTimeout: 300.0
    jobSize: 100
    maximumNumberOfProcesses: 32
    minimumNumberOfJobsPerProcess: 4
  excludes_analyse:
    - app/Infrastructure/Services/Generated/*
  checkMissingIterableValueType: false
  checkGenericClassInNonGenericObjectType: false
